{
  "version": 2,
  "buildCommand": "npm install && cd .. && npm install && cd backend && npm run build",
  "builds":[
    {
       "src": "src/main.ts",
       "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}