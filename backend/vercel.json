{
  "version": 2,
  "installCommand": "npm install-all",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds":[
    {
       "src": "src/main.ts"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}